
generator client {
  provider = "prisma-client-js"
}

//uncomment this if you have'nt a postgresql db
// datasource db {
//   provider = "sqlite"
//   url      = env("DATABASE_URL")
// }

datasource db {
  provider = "postgresql"
  url   = env("DATABASE_URL")
}


model Record {
  id      String @id @default(uuid())
  player  String
  score   BigInt
}